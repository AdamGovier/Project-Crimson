#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/adam_7ep0/Documents/University/Crimson/Crimson Project/dist/node_modules/.pnpm/typeorm@0.3.22_mongodb@5.9._3d7aa5c73427333062954255835b739b/node_modules/typeorm/node_modules:/mnt/c/Users/adam_7ep0/Documents/University/Crimson/Crimson Project/dist/node_modules/.pnpm/typeorm@0.3.22_mongodb@5.9._3d7aa5c73427333062954255835b739b/node_modules:/mnt/c/Users/adam_7ep0/Documents/University/Crimson/Crimson Project/dist/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/adam_7ep0/Documents/University/Crimson/Crimson Project/dist/node_modules/.pnpm/typeorm@0.3.22_mongodb@5.9._3d7aa5c73427333062954255835b739b/node_modules/typeorm/node_modules:/mnt/c/Users/adam_7ep0/Documents/University/Crimson/Crimson Project/dist/node_modules/.pnpm/typeorm@0.3.22_mongodb@5.9._3d7aa5c73427333062954255835b739b/node_modules:/mnt/c/Users/adam_7ep0/Documents/University/Crimson/Crimson Project/dist/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-commonjs.js" "$@"
fi
